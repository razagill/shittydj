{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "no-consecutive-blank-lines": [
      false
    ],
    "no-empty": false,
    "max-line-length": [
      false
    ],
    "no-console": [false],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": [
      false
    ],
    "one-line": [
      false
    ],
    "arrow-parens": [
      false
    ],
    "variable-name": [
      false
    ],
    "member-ordering": [false],
    "no-var-requires": false,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}]
  },
  "rulesDirectory": []
}
